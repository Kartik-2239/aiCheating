{
"name": "ai-cheating",
"version": "1.0.0",
"main": "Electron/main.js",
"type":"module",
"scripts": {
  "dev": "vite",             
  "start": "electron .",
  "build": "vite build && electron-builder"
},
"build": {
  "appId": "com.kartik.AiCheating",
